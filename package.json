{
  "name": "java-to-javascript",
  "version": "0.1.1",
  "description": "Convert Java Classes to ES6 Classes",
  "license": "MIT",
  "bin": "cli.js",
  "module": "index.js",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wyattades/java-to-js"
  },
  "scripts": {
    "examples": "bash -c 'for file in ./examples/*.java; do node ./cli.js $file $([[ \"$file\" = *\"p5\"* ]] && echo --p5); done'"
  },
  "dependencies": {
    "commander": "^2.17.1",
    "java-parser": "0.0.2",
    "js-beautify": "^1.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}

{
  "name": "java-to-javascript",
  "version": "0.1.10",
  "description": "Convert Java Classes to ES6 Classes",
  "keywords": [
    "java",
    "convert",
    "js",
    "javascript",
    "ast",
    "class",
    "es6"
  ],
  "license": "MIT",
  "module": "index.js",
  "main": "build/java-to-javascript.min.js",
  "bin": "cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wyattades/java-to-javascript"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "gulp build",
    "examples": "bash -c 'for file in ./examples/*.java; do node ./cli.js $file $([[ \"$file\" = *\"p5\"* ]] && echo --p5); done'"
  },
  "dependencies": {
    "commander": "^2.17.1",
    "js-beautify": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2",
    "envify": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
